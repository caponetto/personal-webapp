  "scripts": {
    "lint": "yarn eslint ./src --ext .ts,.tsx",
    "build:dev": "rimraf dist && webpack --env dev --mode development",
    "build:prod": "yarn lint && rimraf dist && webpack --mode production",
    "start": "webpack serve --env dev --mode development",
    "format": "prettier --write . --ignore-path .gitignore",
    "format:check": "prettier --check .",
    "cy:open": "yarn cypress open --project cypress",
    "cy:run": "yarn cypress run -b chrome --project cypress",
    "cy:test": "yarn start-server-and-test start https-get://localhost:9001 cy:run",
    "jest:run": "jest --silent",
    "test": "rimraf dist-test && yarn jest:run && yarn cy:test",
    "prepare": "husky install"
