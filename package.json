  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/nunito": "^5.2.7",
    "@fontsource/quicksand": "^5.2.10",
    "@mui/icons-material": "^7.3.8",
    "@mui/lab": "^7.0.1-beta.22",
    "@mui/material": "^7.3.7",
    "@mui/system": "^7.3.7",
    "date-fns": "^4.1.0",
    "i18next": "^25.8.7",
    "i18next-browser-languagedetector": "^8.2.1",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-i18next": "^16.5.4",
    "react-router": "^7.13.0",
    "react-router-dom": "^7.13.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@cypress/code-coverage": "^4.0.0",
    "@jest/types": "^30.2.0",
    "@percy/cli": "^1.31.8",
    "@percy/cypress": "^3.1.7",
    "@size-limit/file": "^12.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-istanbul": "^7.0.1",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.3",
    "cypress": "^15.10.0",
    "dependency-cruiser": "^17.3.8",
    "eslint": "^9.0.0",
    "eslint-plugin-cypress": "^5.3.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "html-minimizer-webpack-plugin": "^5.0.4",
    "html-replace-webpack-plugin": "^2.6.0",
    "html-webpack-plugin": "^5.6.6",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "json-minimizer-webpack-plugin": "^5.0.1",
    "knip": "^5.83.1",
    "lint-staged": "^15.2.10",
    "nyc": "^17.1.0",
    "prettier": "^3.8.1",
    "pretty-quick": "^4.2.2",
    "rimraf": "^6.1.3",
    "size-limit": "^12.0.0",
    "source-map-loader": "^5.0.0",
    "start-server-and-test": "^2.1.3",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "webpack": "^5.105.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.3",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "build:dev": "rimraf dist && webpack --env dev --mode development",
    "build:prod": "npm run check:locales && npm run lint && rimraf dist && webpack --mode production",
    "check:format": "prettier --check . --ignore-path .gitignore",
    "check:locales": "node scripts/check-locales.mjs",
    "check:staged": "lint-staged",
    "check:types": "tsc --noEmit",
    "check:types:cypress": "tsc -p cypress/tsconfig.json",
    "check:deps:unused": "knip --cache --dependencies --exclude unlisted,unresolved,binaries,catalog",
    "check:deps:boundaries": "depcruise --config scripts/dependency-cruiser.cjs src --output-type err-long",
    "check:size": "npm run build:prod && size-limit",
    "coverage:clean": "rimraf .nyc_output dist-test/coverage-combined dist-test/coverage/coverage-combined.json",
    "coverage:combined": "npm run coverage:clean && npm run test && npm run e2e:run:coverage && npm run coverage:merge",
    "coverage:check": "nyc check-coverage --temp-dir .nyc_output/merged",
    "coverage:merge": "node scripts/merge-coverage.mjs",
    "e2e:open:raw": "cypress open",
    "e2e:run:raw": "cypress run",
    "cypress:open": "npm run e2e:open:raw",
    "cypress:run": "npm run e2e:run:raw",
    "e2e:open": "start-server-and-test start tcp:9000 e2e:open:raw",
    "e2e:run": "start-server-and-test start tcp:9000 e2e:run:raw",
    "e2e:run:coverage": "start-server-and-test start:coverage tcp:9000 e2e:run:raw",
    "format": "prettier --write . --ignore-path .gitignore",
    "test:unit": "jest --silent --cacheDirectory .cache/jest",
    "jest:run": "npm run test:unit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs",
    "prepare": "husky",
    "size": "npm run check:size",
    "start": "webpack serve --env dev --mode development",
    "start:coverage": "CYPRESS_COVERAGE=true webpack serve --env dev --mode development",
    "test": "rimraf dist-test && npm run test:unit",
    "test:visual": "start-server-and-test start tcp:9000 \"percy exec -- cypress run --config-file cypress.percy.config.ts --spec cypress/percy/visualRegression.cy.ts\"",
    "verify": "npm run check:types && npm run check:types:cypress && npm run check:locales && npm run check:format && npm run test"
  }
}
